<h1>Products</h1>

<table>
  <tr>
    <th>Image</th>
    <th>Title</th>
    <th>Barcode</th>
    <th>Last Modified</th>
  </tr>

  <% @products.each do |product| %>
    <tr>
      <td><img src='<%= product.image %>'/></td>
      <td><%= product.title %></td>
      <td><%= product.barcode %></td>
      <td><%= product.updated_at %></td>
    </tr>
  <% end %>
</table>
